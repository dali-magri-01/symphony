{% extends 'base.html.twig' %}

{% block title %}Devise{% endblock %}

{% block body %}
    <h1>Devise</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ devise.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ devise.code }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ devise.status }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ devise.createdAt ? devise.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ devise.updatedAt ? devise.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_devise_index') }}">back to list</a>

    <a href="{{ path('app_devise_edit', {'id': devise.id}) }}">edit</a>

    {{ include('devise/_delete_form.html.twig') }}
{% endblock %}
