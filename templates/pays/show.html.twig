{% extends 'base.html.twig' %}

{% block title %}Pays{% endblock %}

{% block body %}
    <h1>Pays</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pay.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ pay.code }}</td>
            </tr>
            <tr>
                <th>Alpha2</th>
                <td>{{ pay.alpha2 }}</td>
            </tr>
            <tr>
                <th>Alpha3</th>
                <td>{{ pay.alpha3 }}</td>
            </tr>
            <tr>
                <th>Nom_en_gb</th>
                <td>{{ pay.nomEnGb }}</td>
            </tr>
            <tr>
                <th>Nom_fr_fr</th>
                <td>{{ pay.nomFrFr }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ pay.createdAt ? pay.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ pay.updatedAt ? pay.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pays_index') }}">back to list</a>

    <a href="{{ path('app_pays_edit', {'id': pay.id}) }}">edit</a>

    {{ include('pays/_delete_form.html.twig') }}
{% endblock %}
